.slicer-gradient .text {
  position: absolute;
  font-size: 8rem;
  color: #fff;
  line-height: 8rem;
  opacity: 0;
  font-family: "Anton", sans-serif;
  transform: translate(-50%, -50%);
  mix-blend-mode: screen;
}
.slicer-gradient .text:nth-child(1) {
  color: #fd3203;
  clip-path: polygon(0% 0%, 0% 50%, 10% 50%);
  transform-origin: 11% -39%;
  animation: fly1 5000ms 0ms cubic-bezier(0.36, 0.1, 0.16, 1) infinite alternate;
}
@keyframes fly1 {
  0% {
    opacity: 0;
    transform: translate(-50%, -50%) rotateX(-25deg) rotateY(-105deg) rotateZ(299deg) translateZ(-1827px);
  }
  10% {
    opacity: 0;
    transform: translate(-50%, -50%) rotateX(-25deg) rotateY(-105deg) rotateZ(299deg) translateZ(-1827px);
  }
  90% {
    opacity: 1;
    transform: translate(-50%, -50%) rotate(0deg) rotateY(0deg) rotateZ(0deg) translateZ(0px);
  }
  100% {
    opacity: 1;
    transform: translate(-50%, -50%) rotate(0deg) rotateY(0deg) rotateZ(0deg) translateZ(0px);
  }
}
